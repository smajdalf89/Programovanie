submenu5.add_command(label='Dievca', command=tritisicmetrovD)
submenu6 = Menu(vysledky_menu)
submenu6.add_command(label='Chlapec', command=hodgranatomCH)
submenu6.add_command(label='Dievca', command=hodgranatomD)
submenu7 = Menu(vysledky_menu)
submenu7.add_command(label='Chlapec', command=skokdopieskuCH)
submenu7.add_command(label='Dievca', command=skokdopieskuD)
submenu8 = Menu(vysledky_menu)
submenu8.add_command(label='Chlapec', command=sedlahCH)
submenu8.add_command(label='Dievca', command=sedlahD)
submenu9 = Menu(vysledky_menu)
submenu9.add_command(label='Chlapec', command=veslovanieCH)
submenu9.add_command(label='Dievca', command=veslovanieD)
submenu10 = Menu(vysledky_menu)
submenu10.add_command(label='Chlapec', command=klukyCH)
submenu10.add_command(label='Dievca', command=klukyD)
submenu11 = Menu(vysledky_menu)
