def hodmedicimbalom():
    meno =simpledialog.askstring("Vložte meno", "prosím zadajte meno žiaka")
    hodmedicimbalom=[]
    trieda =simpledialog.askstring("Trieda", "Zadaj triedu")
    hodmedicimbalom.append(trieda)
    pohlavie =simpledialog.askstring("Pohlavie","Zadaj buď CH alebo D")
    hodmedicimbalom.append(pohlavie)
    vysledok =simpledialog.askfloat("Výsledok", "Zadaj výsledok výkonu")
    hodmedicimbalom.append(vysledok)
    medicimbal[meno] = hodmedicimbalom

medicimbal=dict()

