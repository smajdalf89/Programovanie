PICKLE_FILE = 'vsetko.pickle'
vsetko = (medicimbal, zhyb, zhybyChalani, clnkovyb, stoM, tisicM, tritisicM,
          hodG, skokdoP, sedL, veslp, kluk, preskok, skokdoD)

def print_dicts():
    for d in vsetko:
        print(d)
    print('-'*10)

def ulozit_pickle():
    print_dicts()
    with open(PICKLE_FILE, 'wb') as f:
        pickle.dump(vsetko, f, protocol=-1)

def load_pickle():
    print_dicts()  # before
    with open(PICKLE_FILE, 'rb') as f:
        dictlist = pickle.load(f)
    for i, d in enumerate(dictlist):
        vsetko[i].update(d)
    print_dicts()  # after

file_menu.add_separator()
file_menu.add_command(label='Ulozit subor', command=ulozit_pickle,  accelerator='Ctrl+U', compound='left', underline=0,)
file_menu.add_command(label='Nahrat Subor', command=load_pickle, accelerator='Ctrl+N', compound='left', underline=0, )
file_menu.add_separator()
file_menu.add_command(label='Exit', accelerator='Alt+F4', command=exit_editor)
menu_bar.add_cascade(label='Hlavne menu', menu=file_menu)

