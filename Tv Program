def hodnoteniehodmedicimbalom():
    tkinter.messagebox.showerror(title='Hod medicimbalom', message='Hodnotenie Chlapci:        Hodnotenie Dievcata'
                                                                   '\n 9 m    =1                             7 m    =1'
                                                                   '\n 8 m    =2                             6 m    =2'
                                                                   '\n 7-6 m =3                             5-4 m  =3'
                                                                   '\n 5 m    =4                             3 m    =4'
                                                                   '\n 4-0 m =5                             2-0 m  =5')

