def statistikaskokdodialky():
    sko = str()
    for key, value in sorted(skokdoD.items(), key=lambda podla: podla[1]):
        sko += f'{key.capitalize()}:{value}\n'
    tkinter.messagebox.showinfo("Skok do dialky",sko )

def statistikapreskokycezlavicku():
    pr = str()
    for key, value in sorted(preskok.items(), key=lambda podla: podla[1]):
        pr += f'{key.capitalize()}:{value}\n'
    tkinter.messagebox.showinfo("Preskoky cez lavicku",pr )

