submenu12.add_command(label='Dievca', command=skokdodialkyD)
vysledky_menu.add_cascade(label='1. Hod medicimbalom', menu=submenu1,)
vysledky_menu.add_cascade(label='2. Vydrž v zhybe-iba dievčata', command=zhybvysledok)
vysledky_menu.add_cascade(label='3. Zhyby - chlapci', command=zhybvyCHsledok)
vysledky_menu.add_cascade(label='4. Člnkovy beh', menu=submenu2)
