def hodvysledokCH():
    hodCh = simpledialog.askfloat('Zadaj výkon CH', "Koľko metrov si hodol?")
    if hodCh >= 9:
        tkinter.messagebox.showinfo("Tvoj výsledok je", "Tvoja znamka je 1!\nVýborne")
    elif hodCh >= 8:
        tkinter.messagebox.showinfo("Tvoj výsledok je", "Tvoja znamka je 2!")
    elif hodCh >= 6:
        tkinter.messagebox.showinfo("Tvoj výsledok je", "Tvoja znamka je 3!")
    elif hodCh >= 5:
        tkinter.messagebox.showinfo("Tvoj výsledok je", "Tvoja znamka je 4!")
    elif hodCh >= 0:
        tkinter.messagebox.showinfo("Tvoj výsledok je", "Tvoja znamka je 5!\nSkýs to znova")

