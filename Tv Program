vysledky_menu = Menu(menu_bar, tearoff=0)
submenu1 = Menu(vysledky_menu)
submenu1.add_command(label='Chlapec', command=hodvysledokCH)   #tu tie def
submenu1.add_command(label='Dievca', command=hodvysledokD)
